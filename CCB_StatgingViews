             WHEN WC_CCB_REG_S_start.UNIT_OF_MEASURE_CODE IN ('CCFC', 'MCFC')
             THEN
                'Corrected'
             WHEN WC_CCB_REG_S_start.UNIT_OF_MEASURE_CODE IN ('CCFU', 'MCFU')
             THEN
