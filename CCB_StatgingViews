--			  - This is for the Auto Pay with No Active SA report.	
--20160428 BN - LARGE METER AUDIT REPORT
CREATE OR REPLACE VIEW WC_CCB_ACCOUNT_DS_V(
INTEGRATION_ID,
DATASOURCE_NUM_ID,
