WC_CCB_BILL_S.ACCOUNT_CID,
WC_CCB_BILL_S.BILL_CYCLE_SCHEDULE_CID,
WC_CCB_ADJ_S.ADJUSTMENT_ID,
WC_CCB_ADJ_S.ADJUSTMENT_TYPE_CODE,
