--METER_ID,
MTR_CHAR_VERSION,
EQUIPMENT_COMMENT,
EQUIPMENT_COUNT,
MTR_EQ_INSTALL_DATE,
EQUIPMENT_ID,
--METER_ID,
REMOVAL_DATE,
MTR_EQ_VERSION,
ITEM_CID,                        --LOOKUP ITEM DIM
DISCONNECTED_LOCATION_CODE,
--SP_ID,
SP_TYPE_CODE,
PREMISE_ID,
