    INTEGRATION_ID
,3000
,SYSDATE
,SYSDATE
,0
,wc_ccb_usr_grp_prof_s.APPLICATION_SERVICE_ID
,wc_ccb_usr_grp_prof_s.EXPIRATION_DATE
,wc_ccb_usr_grp_prof_s.DATA_OWNER
