               FROM wc_ccb_fa_log_s
              WHERE log_type = 'OCRP'
           GROUP BY fa_id) created,         --When the order was taken/created
          (SELECT fa_id, log_dttm
             FROM wc_ccb_fa_log_s
            WHERE log_type = 'EXCM') completed,   --When the job was completed
