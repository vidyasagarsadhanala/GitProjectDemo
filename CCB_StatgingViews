          WC_CCB_FT_S.FT_TYPE_FLAG,
          WC_CCB_FT_S.CURRENT_AMOUNT,
          WC_CCB_FT_S.PAYOFF_AMOUNT,
          WC_CCB_FT_S.CREATE_DATE,
