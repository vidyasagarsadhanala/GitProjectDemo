          WC_CCB_SA_S.NBB_AUTOPAY_FLAG,
          WC_CCB_SA_S.SA_DATA_AREA,
          WC_CCB_SA_S.DIVISION_CID SA_DIVISION_CID,
          WC_CCB_SA_S.ACCOUNT_CID,
          WC_CCB_SA_S.PREMISE_CID,
          WC_CCB_SA_S.CURRENCY_CID SA_CURRENCY_CID,
          --CI_PAY_SEG
