BILL_CYCLE_DESCRIPTION,
BILL_CYCLE_L_VERSION
)
AS
SELECT
INTEGRATION_ID,
DATASOURCE_NUM_ID,
W_INSERT_DT,
W_UPDATE_DT,
W_LOAD_NUMBER,
BILL_CYCLE_CODE,
BILL_CYCLE_VERSION,
BILL_CYCLE_LANGUAGE,
BILL_CYCLE_DESCRIPTION,
BILL_CYCLE_L_VERSION
FROM
	WC_CCB_BILL_CYC_S
;


